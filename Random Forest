clf3 = BalancedRandomForestClassifier(
  n_estimators= 1000,
  replacement=True,
  sampling_strategy='not minority',
  n_jobs=4,
  random_state=42,
  oob_score=True,
)
clf3.fit(train_data2, train_labels2)
print('\nClassifier self-performance')
print(metrics.classification_report(train_labels2, clf3.predict(train_data2), zero_division=0))
